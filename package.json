{
	"name": "smoke-editor",
	"version": "0.8.10",
	"main": "dist/index.js",
	"repository": "git@github.com:Artear/smoke_editor.git",
	"author": "Artear",
	"license": "MIT",
	"scripts": {
		"prebuild": "rm -rf dist",
		"start": "webpack-dev-server --config webpack.config.dev.js",
		"build": "webpack --config webpack.config.dist.js",
		"preversion": "./scripts/check-on-master.sh",
		"version": "yarn run build && yarn run update-changelog",
		"update-changelog": "conventional-changelog -p angular -i CHANGELOG.md -s && git add -f dist CHANGELOG.md",
		"mark-skip-ci": "./scripts/mark-commit-skip-ci.sh",
		"postversion": "yarn run mark-skip-ci && yarn release",
		"release": "yarn run release:push-tags && yarn run release:github-release",
		"release:push-tags": "git push && git push --tags",
		"release:github-release": "conventional-github-releaser -p angular",
		"commitmsg": "validate-commit-msg",
		"test": "echo 0"
	},
	"devDependencies": {
		"axios": "^0.18.1",
		"babel": "^6.23.0",
		"babel-cli": "^6.24.1",
		"babel-core": "^6.24.1",
		"babel-loader": "^7.0.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"body-parser": "~1.17.1",
		"bootstrap": "^3.3.7",
		"conventional-changelog-angular": "^1.3.3",
		"conventional-changelog-cli": "^1.3.1",
		"conventional-github-releaser": "^1.1.11",
		"cookie-parser": "~1.4.3",
		"copy-webpack-plugin": "2.1.6",
		"css-loader": "^0.28.1",
		"debug": "~2.6.3",
		"domify": "^1.4.0",
		"express": "~4.15.2",
		"extract-text-webpack-plugin": "^2.1.0",
		"file-loader": "^0.11.1",
		"formidable": "^1.1.1",
		"html-webpack-plugin": "^2.28.0",
		"husky": "^0.14.3",
		"megadraft": "^0.4.24",
		"morgan": "~1.8.1",
		"node-sass": "^4.5.2",
		"path": "^0.12.7",
		"progress-bar-webpack-plugin": "^1.9.3",
		"prop-types": "^15.5.10",
		"react": "^15.5.4",
		"react-autosuggest": "^9.0.1",
		"react-dom": "^15.5.4",
		"react-hot-loader": "next",
		"react-modal-dialog": "^4.0.7",
		"react-resumable-js": "^1.1.23",
		"require-dir": "^0.3.2",
		"sass-loader": "^6.0.5",
		"string": "^3.3.3",
		"style-loader": "^0.17.0",
		"uglifyjs-webpack-plugin": "^0.4.3",
		"url-loader": "^0.5.8",
		"validate-commit-msg": "^2.14.0",
		"webpack": "^2.5.1",
		"webpack-dev-middleware": "^1.10.2",
		"webpack-dev-server": "^2.4.5",
		"webpack-hot-middleware": "^2.18.0"
	},
	"dependencies": {
		"react-responsive-modal": "^2.0.0"
	}
}
